{
  "name": "Download Videos",
  "description": "Download (or open in new tab) any video found in the current page.",
  "version": "0.2.1",
  "author": "Bogdan Dimofte",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "downloads",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": {
    "128": "assets/video_download_blue.png"
  }
}
